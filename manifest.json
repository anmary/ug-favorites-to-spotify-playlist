{
  "manifest_version": 2,
  "name": "UG Favorites to Spotify",
  "description": "Create a playlist on Spotify from your Ultimate Guitar favorites.",
  "version": "1.0",
  "content_scripts": [ {
    "matches": [ "http://my.ultimate-guitar.com/main.php?mode=favorites" ],
    "js": [ "jquery-2.1.4.min.js", "content.js"]
  }],
  "permissions": [
   "activeTab"
   ]
}