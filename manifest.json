{
  "manifest_version": 2,
  "version": "1.1.2",
  "icons": { "128": "icon128.png" },
  "name": "Utimate Guitar favorites to Spotify",
  "description": "Creates a Spotify playlist based on one Ultimate Guitar favorites",
  "content_scripts": [ {
    "matches": [ "http://my.ultimate-guitar.com/main.php?mode=favorites" ],
    "js": [ "jquery-2.1.4.min.js", "content.js"]
  }],
  "permissions": [
   "activeTab"
   ]
}